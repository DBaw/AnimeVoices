<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Background" Color="#272537"/>
    <SolidColorBrush x:Key="OnBackground" Color="#F0F0F0"/>

    <SolidColorBrush x:Key="SearchTextBox" Color="#353340"/>
    <SolidColorBrush x:Key="OnSearchTextBoxEnabled" Color="#CFCFCF"/>
    <SolidColorBrush x:Key="OnSearchTextBoxDisabled" Color="DarkGray"/>

    <SolidColorBrush x:Key="ClickMenuButton" Color="#22202f"/>
    <SolidColorBrush x:Key="HoverOnMenuItem" Color="#302740"/>

    
    
    
    
    <SolidColorBrush x:Key="ExitButtonColor" Color="DarkRed"/>
    <SolidColorBrush x:Key="OnExitButtonColor" Color="LightYellow"/>

    <SolidColorBrush x:Key="MinimizeButtonColor" Color="Yellow"/>
    <SolidColorBrush x:Key="OnMinimizeButtonColor" Color="Black"/>
</ResourceDictionary>